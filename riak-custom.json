<Chain "curl_json_riak_custom">
    <Rule "rewrite_curl_json_to_riak">
        <Match regex>
            Plugin "^curl_json$"
            PluginInstance "^riak.*$"
        </Match>
        <Target "replace">
            PluginInstance "^riak" ""
        </Target>
        <Target "set">
            MetaData "stackdriver_metric_type" "custom.googleapis.com/riak/%{type_instance}"
            Plugin "riak"
        </Target>
    </Rule>
    <Rule "rewrite_empty_plugininstance">
        <Match regex>
            PluginInstance "^$"
        </Match>
        <Target "set">
            PluginInstance "localhost"
        </Target>
    </Rule>
    <Rule "go_back">
        Target "return"
    </Rule>
</Chain>

<Chain "PreCache">
    <Rule "jump_to_curl_json_riak_custom">
        <Target "jump">
            Chain "curl_json_riak_custom"
        </Target>
    </Rule>
</Chain>
